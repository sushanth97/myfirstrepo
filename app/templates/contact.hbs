
<h1>List of contacts</h1>
<table class="table table-hover width:50px">
  
  <tbody>
      {{#each model as |person|}}
   
    <tr>
      <td>{{person.id}}</td>
      {{#link-to "contact.details" person.id}}
      <td>{{person.name}}</td>
      {{/link-to}}
    </tr>

    {{/each}}
    </tbody>
    </table>
    {{#link-to "new"}}new{{/link-to}}

{{outlet}}